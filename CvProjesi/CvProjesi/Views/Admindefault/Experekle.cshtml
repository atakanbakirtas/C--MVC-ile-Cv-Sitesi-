
@{
    ViewBag.Title = "Experekle";
    Layout = "~/Views/Shared/AdminMaster.cshtml";
}

<h2 class="text-center">Yetenek Ekle</h2>
<form class="form-group" method="post">
    <div class="mb-3">
        <input name="UserID" type="text" class="form-control" required="" placeholder="Kullanıcı ID" />
    </div>

    <div class="mb-3">
        <input name="CompanyName" type="text" class="form-control" required="" placeholder="Firma adı" />
    </div>

    <div class="mb-3">
        <input name="JobTitle" type="text" class="form-control" required="" placeholder="Görev" />
    </div>

    <div class="mb-3">
        <input name="StartDate" type="date" class="form-control" required="" placeholder="Başlangıç" />
    </div>

    <div class="mb-3">
        <input name="EndDateDATE" type="date" class="form-control" required="" placeholder="Bitiş" />
    </div>

    <div class="d-grid">
        <button type="submit" class="btn btn-info btn-lg" id="btn">Deneyim Ekle</button>
    </div>
</form>

